{
    "name": "codix/cdx-agent",
    "description": "CDX Agent - Remote monitoring and control agent for Laravel applications",
    "type": "library",
    "license": "proprietary",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Codix",
            "email": "dev@codix.io"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Codix\\CdxAgent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codix\\CdxAgent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codix\\CdxAgent\\CdxAgentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
